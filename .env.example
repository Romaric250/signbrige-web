# Site URL (for SEO, sitemap, canonical URLs)
# Set to your production domain e.g. https://signbridge.com
NEXT_PUBLIC_SITE_URL=https://signbridge.com

# API (for dashboard/auth)
NEXT_PUBLIC_API_URL=http://localhost:3001/api

# Web app (SignNova) - runs on subdomain e.g. app.signbridge.com
# Get Started and Open SignNova buttons link here
NEXT_PUBLIC_WEB_APP_URL=https://app.signbridge.com
